{
  "name": "pyrax-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start --port 3000",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "prepare": "husky install",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "send-admin-invite": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} scripts/send-admin-invite.ts",
    "workers": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} src/workers/index.ts",
    "workers:status": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} src/workers/status-worker.ts",
    "workers:email": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} src/workers/email-worker.ts",
    "workers:incident": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} src/workers/incident-worker.ts",
    "workers:build": "tsc src/workers/*.ts --outDir dist/workers --esModuleInterop --resolveJsonModule",
    "deploy": "npm run build && npm run db:push"
  },
  "prisma": {
    "seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.956.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@rainbow-me/rainbowkit": "^2.2.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@tanstack/react-query": "^5.59.0",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.12.0",
    "clsx": "^2.1.1",
    "ioredis": "^5.4.1",
    "lucide-react": "^0.460.0",
    "next": "^15.0.3",
    "pino-pretty": "^13.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.6.0",
    "viem": "^2.21.32",
    "wagmi": "^2.12.25",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@prisma/client": "^5.22.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.0.3",
    "husky": "^9.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.49",
    "prisma": "^5.22.0",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
